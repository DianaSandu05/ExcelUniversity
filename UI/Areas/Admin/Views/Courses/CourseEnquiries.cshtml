
@{
    ViewBag.Title = "CourseEnquiries";
    Layout = "~/Areas/Admin/Views/Shared/UserLayout.cshtml";
}
@model List<DTO.InquiriesDTO>
<section class="content-header">
    <h1>Inquiries List</h1>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item">Admin</li>
        <li class="breadcrumb-item">Course</li>
        <li class="breadcrumb-item active">Course List</li>
    </ol>
</section>
<br />
<div class="card-footer">
    <button type="submit" class="btn-primary" onclick="window.location.here='Admin/User/AddUser'">Add Course</button>
</div>
<div class="card">
    <div class="card-body">
        <table id="ShowTable" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Course Name</th>
                    <th>Start Date</th>
                    <th>End date</th>
                    <th> Delete </th>
                    <th> Update</th>
                </tr>
            </thead>
            @foreach (var item in Model)
            {
                <tbody>
                    <tr id="a_@item.ID">
                        <td>@item.CourseName</td>
                        <td>@item.StudentID</td>
                        <td>@item.sFirstName @item.sSurname</td>
                        <td align="center" valign="middle">
                            <a href="/Admin/User/UpdateUser/@item.ID"></a>
                        </td>
                    </tr>
                </tbody>
            }
            <tfoot>
                <tr>
                    <th>Course Name</th>
                    <th>Start Date</th>
                    <th>End date</th>
                    <th> Delete </th>
                    <th> Update</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>

